{
	"name": "voxonics/data-structure",
	"type": "library",
	"description": "Implementation of data structure algorithms, like LinkedList, Tree",
	"license": "MIT",
	"authors": [
        {
            "name": "D.Minakov",
            "email": "aikrofstark@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.2"
    },
    "require-dev": {
    	"phpunit/phpunit": "^8.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },

    "autoload": {
        "psr-4": {
            "Voxonics\\Data\\Structure\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Voxonics\\Data\\Structure\\Tests\\": "tests/"
        }
    }
}